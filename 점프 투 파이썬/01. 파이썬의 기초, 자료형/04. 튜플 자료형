# 튜플의 모습
t1 = ()
t2 = (1,)
t3 = (1,2,3)
t4 = 1,2,3
t5 = ('a', 'b',('ab', 'cd'))

# 튜플 요솟값을 삭제하려 할 때
# t1 = (1, 2, 'a', 'b')
# del t1[0] # 에러 뜸

# 인덱싱 하기
t1 = (1,2,'a','b')
print(t1[0])

# 슬라이싱 하기
t1 = (1,2,'a','b')
print(t1[1:])

#튜플 더하기
t1 = (1,2,'a','b')
t2 = (3,4)
print(t1 + t2)

# 튜플 곱하기
t2 = (3, 4)
print(t2 * 3)

# 튜플의 길이 구하기
t1 = (1,2,'a','b')
print(len(t1))



